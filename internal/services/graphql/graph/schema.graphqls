type Mutation {
  signUp(input: AuthData!): AuthPayload!
  signIn(input: AuthData!): AuthPayload!
}

type Query {
  getUserInfo(input: UserAccountData!): UserAccountPayload!
}

input UserAccountData {
  userId: String!
}

type UserAccountPayload {
  username: String!
}

input AuthData {
  username: String!
  password: String!
}

type AuthPayload {
  userId: String!
  role: String!
}
